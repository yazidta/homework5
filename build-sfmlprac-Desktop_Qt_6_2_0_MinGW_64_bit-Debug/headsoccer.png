<script>
    // Separates the download counters with kommas
    var monthlyDownloads = "";
    monthlyDownloads = monthlyDownloads.replace(new RegExp("^(\\d{" + (monthlyDownloads.length % 3 ? monthlyDownloads.length % 3 : 0) + "})(\\d{3})", "g"), "$1 $2").replace(/(\d{3})+?/gi, "$1 ").trim().replace(/\s/g, ",");
    $("#downloads-monthly").text(monthlyDownloads);

    var totalDownloads = "";
    totalDownloads = totalDownloads.replace(new RegExp("^(\\d{" + (totalDownloads.length % 3 ? totalDownloads.length % 3 : 0) + "})(\\d{3})", "g"), "$1 $2").replace(/(\d{3})+?/gi, "$1 ").trim().replace(/\s/g, ",");
